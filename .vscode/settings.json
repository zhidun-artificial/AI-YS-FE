{
  "editor.formatOnSave": true,
  "prettier.requireConfig": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.enable": true,
  "eslint.codeActionsOnSave.mode": "problems",
  "cSpell.words": ["cmaps", "dify", "umijs"],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "[svg]": {
    "editor.defaultFormatter": "jock.svg"
  }
}
