{
  "editor.formatOnSave": true,
  "prettier.requireConfig": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.enable": true,
  "eslint.codeActionsOnSave.mode": "problems",
  "cSpell.words": [
    "antdx",
    "cmaps",
    "dify",
    "favicons",
    "Popconfirm",
    "umijs",
    "Usergroup"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "[svg]": {
    "editor.defaultFormatter": "jock.svg"
  }
}
